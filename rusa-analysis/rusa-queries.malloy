import "./rusa.malloy"

// --
// Name: Club-specific results
query: event_results -> results_histogram + {
  where:
    events.routes.club ~ r'San Francisco'
    and events.event_date ? '2011-01-01' to '2022-01-01'
}

// --
// Name: Route-specific results
query: event_results -> results_histogram + {
  where:
    events.routes.distance = 600
    and events.event_date ? '2011-01-01' to '2022-01-01'
}